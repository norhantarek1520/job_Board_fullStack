<!DOCTYPE html>
<html lang="en">
        <%- include('component/head') -%>
<body> 
        <%- include('component/header') -%>
    
        <!-- bradcam_area  -->
        <div class="bradcam_area bradcam_bg_1">
            <div class="container">
                <div class="row">
                    <div class="col-xl-12">
                        <div class="bradcam_text">
                            <h3> Edit Profile </h3>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--/ bradcam_area  -->
        <% users.forEach( user => { %>
        <div class="container">
            <div class="row gutters">
                <div class="col-xl-3 col-lg-3 col-md-12 col-sm-12 col-12">
                    <div class="card h-100">

                        <div class="card-body">
                            <div class="account-settings">

                                <div class="user-profile">

                                    <div class="user-avatar">
                                        <% if(user.gender == "female") {%>
                                            <!-- <img src="data:image/jpeg;base64," alt="avatar"
                                            class="rounded-circle img-fluid" style="width: 150px;"> -->
                                            <img src="https://cdn3.iconfinder.com/data/icons/avatars-business-human1/180/woman1-512.png" alt="avatar"
                                            class="rounded-circle img-fluid" style="width: 150px;">
                                            <% }else {%>
                                            <img src="https://mdbcdn.b-cdn.net/img/Photos/new-templates/bootstrap-chat/ava3.webp" alt="avatar"
                                            class="rounded-circle img-fluid" style="width: 150px;">
                                            <% }%>
                                    
                                    </div>
                                    <h5 class="user-name"> <%= user.name %> </h5>
                                    <h6 class="user-email"> <%= user.email %> </h6>
                                </div>
                                
                            </div>
                        </div>

                    </div>
                </div>
                <div class="col-xl-9 col-lg-9 col-md-12 col-sm-12 col-12">
                <div class="card h-100">


                    <div class="card-body">
                        <div class="row gutters">
                            
                            <form   action="/editProfile%>" method="POST" >
                                <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                                    <h2 class="mb-2 text-primary">Personal Details</h2>
                                </div>

                                <div class="form-outline mb-4">
                                    <label for="fullName">Full Name</label>
                                    <input type="text" class="form-control form-control-lg"id="fullName" placeholder="Enter full name" name ="name" value="<%= user.name %>">
                                </div>
                            
                                <div class="form-outline mb-4">
                                    <div class="form-group">
                                        <label for="eMail">Email</label>
                                        <input type="email" class="form-control form-control-lg"id="eMail" placeholder="Enter email ID" name="email" value="<%= user.email %>">
                                    </div>
                                </div>
                            
                                <div class="form-outline mb-4">
                                    <div class="form-group">
                                        <label for="age">Age	</label>
                                        <input type="text" class="form-control form-control-lg"id="age" placeholder="Enter age" name="age" value="<%= user.age%>">
                                    </div>
                                </div>
                                
                                <div class="form-outline mb-4">
                                    <div class="form-group">
                                        <label for="gender">Gender</label>
                                        <input type="text" class="form-control form-control-lg"id="gender" placeholder="Enter gender" name="gender" value="<%= user.gender %>">
                                    </div>
                                </div>
                                
                                <div class="form-outline mb-4">
                                    <div class="form-group">
                                        <label for="job_title">Job title</label>
                                        <input type="text" class="form-control form-control-lg"id="job_title" placeholder="Enter job_title" name="job_title" value="<%= user.job_title %>">
                                    </div>
                                </div>
                            
                                <div class="form-outline mb-4">
                                    <div class="form-group">
                                        <label for="education">Education</label>
                                        <input type="text" class="form-control form-control-lg"id="education" placeholder="Enter education" name="education" value="<%= user.education %>">
                                
                                </div>
                            
                                <div class="form-outline mb-4">
                                
                                        <label for="phone">Phone</label>
                                        <input type="text" class="form-control form-control-lg"id="phone" placeholder="Enter phone number" name="phone_number"  value="<%= user.phone_number %>">
                                    
                                </div>
                                
                                <div class="form-outline mb-4">                           
                                        <label for="Street">Address</label>
                                        <input type="name" class="form-control form-control-lg"id="Street" placeholder="Enter Street" name="address" value="<%= user.address %>">
                                </div>
                                
                                <div class="form-outline mb-4">
                                    <div class="text-center">
                                        <!-- <button type="submit" id="submit" name="submit" class="btn btn-secondary">Cancel</button> -->
                                        <button type="submit" id="submit" name="submit" class="btn btn-primary">Update</button>
                                    </div>
                                </div>
                        
                            </form>


                        </div>
                    
                    </div>
                </div>
                </div>
            </div>
        </div>
        <% }) %>
 
        <%- include('component/footer') -%>
        <%- include('component/include_files') -%>

</body>
</html>